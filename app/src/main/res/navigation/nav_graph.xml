            app:destination="@id/formulario" />
    </fragment>
    <fragment
        android:id="@+id/formulario"
        android:name="com.example.recepcioncda.view.ui.fragments.FormularioFragment"
        android:label="fragment_formulario"
        tools:layout="@layout/fragment_formulario" />
